@model Backend.Models.ViewModels.CityViewModel


<tr class="city">
    <td>
        @Model.City.Name
    </td>
    <td>
        @Model.Country.Name
    </td>
    <td>

        <div id="deleteForm">
            <a asp-action="Edit" asp-route-id="@Model.City.CityId">Edit</a>
            <a asp-action="Delete" asp-route-id="@Model.City.CityId">Delete</a>
        </div>

    </td>

</tr>